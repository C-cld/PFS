<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyy.dao.AccountMapper">
	<select id="find" resultType="com.cyy.domain.Account">
		select name,username,password,url from account where name like "%"#{1}"%"
	</select>

	<insert id="add" parameterType="com.cyy.domain.Account" keyProperty = "id">
		insert into account (id,name,username,password,url) values(#{id},#{name},#{username},#{password},#{url});
	</insert>
</mapper>